{
  "cpp gcc":{
    "folder":"cpp",
    "build":"g++ -O3 -march=native -flto -DNDEBUG <src> -o <out>"
  },
  "--cpp clang":{
    "folder":"cpp",
    "build":"clang++ -O3 -march=native -flto -DNDEBUG <src> -o <out>"
  },
  "--elixir onecore":{
    "folder":"elixir",
    "buildSize":"wc -c < /tmp/Elixir.Main.beam",
    "build":"elixirc --no-debug-info -o /tmp <src> ",
    "run":"elixir --erl \"+S 1 +A 1 +sbt u +sub true -smp enable\" -pa /tmp -e \"Main.main()\" -- "
  },
  "--elixir":{
    "folder":"elixir",
    "multicore":true,
    "build":"elixirc --no-debug-info <src>",
    "//":"'out' is used ONLY for measuring the file size.",
    "out":"Elixir.Main.beam",
    "run":"elixir --erl \"+S $(nproc) +A 1 +sbt u +sub true -smp enable\" -e \"Main.main()\" -- "
  },
  "--haskell":{
    "folder":"hs",
    "ext":"hs",
    "build":"stack ghc -- -Wno-x-partial -O2 -rtsopts -with-rtsopts=\"-A32m -H32m\" <src> -o <out>"
  },
  "--rust":{
    "folder":"rust",
    "build":"rustc -C opt-level=3 -C target-cpu=native -C lto=fat -C codegen-units=1 -C panic=abort <src> -o <out>"
  },
  "--python":{
    "folder":"py",
    "run":"python -OO -B -E -u <src>"
  },
  "--pypy3":{
    "folder":"py",
    "run":"pypy3 -OO -B -E -u <src>"
  },
  "--perl":{
    "folder":"perl",
    "run":"perl <src>"
  },
  "--nodeJS":{
    "folder":"js",
    "run":"node --no-concurrent-sweeping --predictable --max-old-space-size=4096 <src>"
  },
  "--denoJS":{
    "folder":"js",
    "build":"deno compile --unstable --quiet --allow-read --allow-write --output <out> <src>"
  },
  "--nim":{
    "folder":"nim",
    "comment":"",
    "build":"nim c -d:release --verbosity:0 --opt:speed --passC:\"-march=native\" --passL:\"-s\"  -o=<out> <src>"
  },
  "--go":{
    "folder":"go",
    "build":"go build -ldflags=\"-s -w\" -trimpath -o <out> <src>"
  }
}